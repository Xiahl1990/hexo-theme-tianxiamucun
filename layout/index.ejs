<% page.posts.each(function(post) { %>
    <%- partial('box/box_article', { post: post, index: true }) %>
<% }); %>
<%- partial('widget/article_scroll_button') %>
<% if (page.total > 1) { %>
    <nav id="page-nav">
        <%- paginator({
            prev_text: '&laquo; ' + __('nav.prev'),
            next_text: __('nav.next') + ' &raquo;'
        }) %>
    </nav>
<% } %>
