<!-- editormd -->
<%- js('vendor/editor/editormd') %>

<!-- analytics -->
<%- partial('plugin/scripts') %>

<!-- Custom Scripts -->
<%- js('js/app') %>

<!-- Init Scripts -->
<script>
  $(function(){

      $("html").niceScroll();

      //jQuery UI sortable for the todo list
      $(".todo-list").sortable({
        placeholder: "sort-highlight",
        handle: ".handle",
        forcePlaceholderSize: true,
        zIndex: 999999
      });

      $(".fancybox-thumb").fancybox({
        prevEffect  : 'none',
        nextEffect  : 'none',
        helpers : {
          title : {
            type: 'inside'
          },
          thumbs  : {
            width : 50,
            height  : 50
          }
        }
      });

      $("#calendar").datepicker({
        todayHighlight: true,
        language: "zh-CN"
      });

  });
</script>
